{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "althinect/filament-spatie-roles-permissions": "3.x-dev",
        "filament/filament": "^4.1.9",
        "laravel/framework": "^12.34",
        "laravel/reverb": "^1.6",
        "laravel/sanctum": "^4.2",
        "laravel/tinker": "^2.10.1",
        "livewire/livewire": ">=3.6.4",
        "nunomaduro/essentials": "@dev",
        "spatie/laravel-activitylog": "^4.10.2",
        "spatie/laravel-permission": "^6.21",
        "typesense/typesense-php": "^5.1"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^2.1.0",
        "fakerphp/faker": "^1.24.1",
        "filament/upgrade": "^4.1.9",
        "laravel/boost": "^1.4.0",
        "laravel/pail": "^1.2.3",
        "laravel/pint": "^1.25.1",
        "laravel/sail": "^1.46",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.2",
        "pestphp/pest": ">=4.1.2",
        "pestphp/pest-plugin-faker": ">=4",
        "pestphp/pest-plugin-laravel": ">=4",
        "pestphp/pest-plugin-livewire": ">=4.0.1",
        "rector/rector": ">=2.2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "update:requirements": [
            "composer bump",
            "npx npm-check-updates -u"
        ],
        "lint": [
            "@php vendor/bin/rector",
            "@php vendor/bin/pint --parallel",
            "npm run lint"
        ],
        "test:lint": [
            "@php vendor/bin/pint --parallel",
            "@php vendor/bin/rector --dry-run",
            "npm run test:lint"
        ],
        "test:unit": "pest --parallel",
        "test": [
            "@test:unit",
            "@test:lint"
        ],
        "fix": [
            "@php vendor/bin/pint --ansi",
            "@php vendor/bin/rector process app --ansi",
            "@php vendor/bin/rector process database --ansi",
            "@php vendor/bin/rector process routes --ansi",
            "@php vendor/bin/rector process tests --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "bump-after-update": true,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
